    "winston-loggly": "^1.0.4"
  },
  "devDependencies": {
    "browser-sync": "^2.6.4",
    "chai": "^2.0.0",
    "complexity-report": "^1.4.0",
    "coveralls": "^2.11.2",
    "del": "~1.1.0",
    "escomplex-js": "^1.2.0",
    "eslint": "^0.20.0",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-changed": "^1.1.1",
    "gulp-concat": "^2.5.1",
    "gulp-coveralls": "^0.1.4",
