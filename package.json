{
  "private": true,
  "version": "1.1.0-8",
  "name": "Freecycle",
  "company": "Freecycle, Inc.",
  "subdomain": "freecycle-app",
  "description": "Freecycle Beta Web Site",
  "author": "Venture Labs",
  "keywords": [
    "freecycle",
    "recycle",
    "environment",
    "green",
    "node",
    "express",
    "passport",
    "bootstrap"
  ],
  "homepage": "http://jfergie.github.io/web",
  "repository": {
    "type": "git",
    "url": "https://github.com/jfergie/web"
  },
  "bugs": "https://github.com/jfergie/web/issues",
  "contributors": "https://github.com/VentureLabsInc/website/graphs/contributors",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.github.com/VentureLabsInc/freecycle/master/LICENSE"
    }
  ],
  "scripts": {
    "start": "node app.js",
    "test": "mocha",
    "test-cov": "mocha -R spec && npm run coveralls",
    "test-coveralls": "mocha -R spec && npm run coveralls",
    "testnew": "mocha -R spec && npm run coverage",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls",
    "//coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "coverage": "npm run analyze-coverage && npm run check-coverage",
    "//": "The following are from https://github.com/ariya/esprima/blob/master/package.json",
    "build": "npm run compile",
    "compile": "npm run compile-full && npm run compile-runtime",
    "generate-regex": "node tools/generate-identifier-regex.js",
    "test2": "node test/run.js && npm run lint && npm run coverage",
    "lint": "npm run check-version && npm run eslint && npm run jscs && npm run complexity",
    "check-version": "node tools/check-version.js",
    "jscs": "jscs app.js",
    "eslint": "node node_modules/eslint/bin/eslint.js app.js",
    "complexity": "node tools/list-complexity.js && cr -s -l -w --maxcyc 17 app.js",
    "analyze-coverage": "istanbul cover test/app_test.js",
    "check-coverage": "istanbul check-coverage --statement 100 --branch 100 --function 100",
    "benchmark": "node test/benchmarks.js",
    "benchmark-quick": "node test/benchmarks.js quick"
  },
  "main": "app.js",
  "engines": {
    "node": ">=0.10.0"
  },
