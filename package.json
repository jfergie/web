    "github-api": "~0.10.3",
    "helmet": "^0.6.1",
    "jade": "^1.9.2",
    "lastfm": "~0.9.2",
    "lodash": "^3.3.0",
    "method-override": "~2.3.1",
    "minimist": "^1.1.1",
    "mocha-lcov-reporter": "0.0.2",
    "moment": "^2.9.0",
    "mongoose": "^4.0.2",
    "morgan": "^1.5.1",
    "node-foursquare": "^0.3.0",
    "nodemailer": "^1.3.1",
    "numeral": "~1.5.3",
