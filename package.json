    "gulp-jade": "^1.0.0",
    "gulp-jscs": "^1.4.0",
    "gulp-jshint": "^1.9.2",
    "gulp-less": "^3.0.0",
    "gulp-livereload": "^3.7.0",
    "gulp-load-plugins": "~0.8.0",
    "gulp-mocha": "^2.0.1",
    "gulp-nodemon": "^1.0.5",
    "gulp-open": "^0.3.2",
    "gulp-rename": "~1.2.0",
    "gulp-size": "^1.2.0",
    "gulp-sourcemaps": "~1.3.0",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.4",
    "imagemin-pngquant": "^4.0.0",
    "istanbul": "^0.3.13",
    "jscomplexity": "^1.1.1",
    "jscs": "^1.13.1",
    "jshint-stylish": "~1.0.0",
    "mocha": "~2.1.0",
    "psi": "^1.0.5",
    "regenerate": "^1.2.1",
