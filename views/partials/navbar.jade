    // LANGUAGE
    .btn-group.pull-right.hidden-xs
      button.dropdown-toggle.language(type='button', data-toggle='dropdown')
        img(src='images/flags/us.png', width='16', height='11', alt='EN Language')
        |  English
        span.caret
      ul.dropdown-menu
        li
          a(href='#')
