            |  [DE] German
        li
          a(href='#')
            img(src='images/flags/fr.png', width='16', height='11', alt='FR Language')
            |  [FR] French
        li
          a(href='#')
            img(src='images/flags/ru.png', width='16', height='11', alt='RU Language')
            |  [RU] Russian
    // /LANGUAGE
    // SIGN IN
    .pull-right.nav.signin-dd
      a#quick_sign_in(href='page-signin.html', data-toggle='dropdown')
        i.fa.fa-users
        span.hidden-xs  Sign In
      .dropdown-menu(role='menu', aria-labelledby='quick_sign_in')
        h4 Sign In
        form(action='page-signin.html', method='post', role='form')
          .form-group
            // email
            input.form-control(required='', type='email', placeholder='Username or email')
          .input-group
            // password
            input.form-control(required='', type='password', placeholder='Password')
            // submit button
            span.input-group-btn
              button.btn.btn-primary Sign In
          .checkbox
            // remember
            label
              input(type='checkbox')
              |  Remember me •
              a(href='page-signin.html') Forgot password?
        hr
        fb:login-button(scope='public_profile,email,user_friends', onlogin='checkLoginState();') Connect With Facebook
        a(href='checkLoginState();')
          i.fa.fa-facebook
          | Connect With Facebook
        a.btn-twitter.fullwidth.radius3(href='#')
          i.fa.fa-twitter
          |  Connect With Twitter
        a.btn-google-plus.fullwidth.radius3(href='#')
          i.fa.fa-google-plus
          |  Connect With Google
        // GOOGLE PLUS START
        span#signinButton
          span.g-signin(data-callback='signinCallback', data-clientid='599405964369-jdok3lour9f11so6fdviqk2lhcjvaisl.apps.googleusercontent.com', data-cookiepolicy='single_host_origin', data-scope='https://www.googleapis.com/auth/plus.login')
        |                     < !-- GOOGLE PLUS END -->
        p.bottom-create-account
